<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.linegrillpresent.studybuddy"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="24"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.INTERNET" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/sb_v3"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.linegrillpresent.studybuddy.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.linegrillpresent.studybuddy.RegisterActivity" />
        <activity android:name="com.linegrillpresent.studybuddy.RegisterNewGroup" />
        <activity android:name="com.linegrillpresent.studybuddy.RegisterNewCourse" />
        <activity android:name="com.linegrillpresent.studybuddy.ShowGroupInfoActivity" />
        <activity android:name="com.linegrillpresent.studybuddy.WaitActivity" />
        <activity android:name="com.linegrillpresent.studybuddy.ShowClassInfoActivity" />
        <activity android:name="com.linegrillpresent.studybuddy.MycourseActivity" />
        <activity android:name="com.linegrillpresent.studybuddy.MygroupActivity" />
        <activity android:name="com.linegrillpresent.studybuddy.MyprofileActivity" />
        <activity android:name="com.linegrillpresent.studybuddy.WelcomePage" />
        <activity
            android:name="com.linegrillpresent.studybuddy.BookingActivity"
            android:label="@string/title_activity_booking"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>